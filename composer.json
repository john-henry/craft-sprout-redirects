{
    "name": "barrelstrength/sprout-redirects",
    "description": "User-friendly Redirect Manager with 404 logging and Multi-Site support",
    "version": "4.0.0",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "seo",
        "redirects"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Barrel Strength",
            "homepage": "https://www.barrelstrengthdesign.com/"
        }
    ],
    "support": {
        "email": "sprout@barrelstrengthdesign.com",
        "issues": "https://github.com/barrelstrength/craft-sprout-redirects/issues?state=open",
        "source": "https://github.com/barrelstrength/craft-sprout-redirects",
        "docs": "https://sprout.barrelstrengthdesign.com/docs/redirects",
        "rss": "https://github.com/barrelstrength/craft-sprout-redirects/commits/v2.atom"
    },
    "require": {
        "craftcms/commerce": "^4.0.0",
        "barrelstrength/sprout-base": "^6.0.0",
        "barrelstrength/sprout-base-redirects": "^1.5.4"
    },
    "autoload": {
        "psr-4": {
            "barrelstrength\\sproutredirects\\": "src/"
        }
    },
    "extra": {
        "name": "Sprout Redirects",
        "handle": "sprout-redirects",
        "developer": "Barrel Strength",
        "developerUrl": "https://barrelstrengthdesign.com",
        "documentationUrl": "https://sprout.barrelstrengthdesign.com/docs/redirects",
        "class": "barrelstrength\\sproutredirects\\SproutRedirects"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main",
        "craftcms/rector": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "scripts": {
        "fix-cs": "ecs check --ansi --fix",
        "check-cs": "ecs check --ansi",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
